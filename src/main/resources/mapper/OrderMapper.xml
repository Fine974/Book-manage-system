<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fine.bookmanagesystem.mapper.OrderMapper">
    <insert id="createOrder" parameterType="com.fine.bookmanagesystem.model.Order">
        INSERT INTO orders (user_id, book_id, quantity, total_price)
        VALUES (#{userId}, #{bookId}, #{quantity}, #{totalPrice})
    </insert>
</mapper>